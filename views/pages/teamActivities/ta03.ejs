<div class="jumbotron text-center">
  <div class="container">
    <h1><%= title %></h1>
    <p>This is the placeholder for Team Activity 03</p>
  </div>
</div>
<div class="container1">
  <% for(let item of body) { %> 
    <div class="item">
      <h2><%= item.name %></h2>
